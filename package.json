{
  "name": "type-inspector",
  "version": "1.0.0",
  "description": "Template for node packages",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "reset-modules": "npx --quiet rimraf node_modules/ package-lock.json",
    "reset-creation": "npx --quiet rimraf dist/ tests/reports/",
    "reset": "npm run reset-creation && npm run reset-modules",
    "init": "npm run reset && npm i && npm run build && npm run test",
    "create": "npm run reset-creation && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maddin1502/type-inspector.git"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "author": "maddin1502",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maddin1502/type-inspector/issues"
  },
  "homepage": "https://github.com/maddin1502/type-inspector#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "jest": "^27.4.3",
    "jest-class-extended": "^1.0.1",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^13.0.0",
    "ts-jest": "^27.1.0",
    "ts-node": "10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@sideway/address": "^4.1.2",
    "ts-lib-extended": "^1.1.4"
  }
}
