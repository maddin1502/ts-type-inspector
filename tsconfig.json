{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
