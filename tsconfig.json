{
  "compilerOptions": {
    "outDir": "dist/",
    "target": "ES6",
    "module": "ES2022",
    "lib": [
      "ES2022"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
